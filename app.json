{
  "expo": {
    "name": "climaApk",
    "slug": "climaapk",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/dia48.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/dia48.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/dia48.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "package": "com.drago_scuro.climaapk"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d2cb0433-6de1-450f-ac66-284e12208e75"
      }
    },
    "owner": "drago_scuro"
  }
}
